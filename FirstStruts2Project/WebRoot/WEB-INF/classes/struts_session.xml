<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
	<package name="session" extends="struts-default" namespace="/">
		<action name="session" class="session.SessionCount">
			<result>/sessionCount.jsp</result>
		</action>
	</package>
	<package name="IoC" extends="struts-default" namespace="/">
		<action name="IoC" class="session.IoCSession">
			<result>/IoCSessionResult.jsp</result>
		</action>
	</package>

</struts>